    "/api/images/upload": {
      "post": {
        "summary": "Upload an image",
        "parameters": [
          { "name": "userId", "in": "body", "required": true, "type": "string" }
        ],
        "responses": {
          "200": { "description": "Image uploaded successfully" }
        }
      }
    }
  }
